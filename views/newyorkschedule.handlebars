<!-- id for New York Activities -->
<p id="startCentral"></p>
<p id="endCentral"></p>
<p id="startStatue"></p>
<p id="endStatue"></p>
<p id="startTop"></p>
<p id="endTop"></p>
<p id="startThe"></p>
<p id="endThe"></p>
<p id="startTimes"></p>
<p id="endTimes"></p>
<!-- New York Food -->
<p id="startFive"></p>
<p id="endFive"></p>
<p id="startJuliana"></p>
<p id="endJuliana"></p>
<p id="startBleecker"></p>
<p id="endBleecker"></p>
<p id="startAmélie"></p>
<p id="endAmélie"></p>
