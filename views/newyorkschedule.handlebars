<p id="endKeens"></p>
<!-- New York Shopping -->
<p id="startBergdorf"></p>
<p id="endBergdorf"></p>
<p id="startGrand"></p>
<p id="endGrand"></p>
<p id="startFifth"></p>
<p id="endFifth"></p>

<script type="text/javascript">
  // variables for New York Activities
  var startCentral = {{Central}};
  var endCentral = {{endCentral}};
  var startStatue = {{Statue}};
  var endStatue = {{endStatue}};
  var startTop = {{Top}};
  var endTop = {{endTop}};
  var startThe = {{The}};
  var endThe = {{endThe}};
  var startTimes = {{Times}};
  var endTimes = {{endTimes}};
  // // New York Food
  var startFive = {{Five}};
  var endFive = {{endFive}};
  var startJuliana = {{Juliana}};
  var endJuliana = {{endJuliana}};
  var startBleecker = {{Bleecker}};
  var endBleecker = {{endBleecker}};
  var startAmélie = {{Amélie}};
  var endAmélie = {{endAmélie}};
  var startKeens = {{Keens}};
  var endKeens = {{endKeens}};
  // //New York Shopping
  var startBergdorf = {{Bergdorf}};
  var endBergdorf = {{endBergdorf}};
  var startGrand = {{Grand}};
  var endGrand = {{endGrand}};
  var startFifth = {{Fifth}};
  var endFifth = {{endFifth}};

  function test() {
    if (startCentral != "0"){
      document.getElementById("startCentral").innerHTML = "Central Park: " + "Start: " + startCentral + " End: " + endCentral
    }
    if (startStatue != "0"){
      document.getElementById("startStatue").innerHTML = "Statue of Liberty: " + "Start: " + startStatue + " End: " + endStatue
    }
    if (startTop != "0"){
      document.getElementById("startTop").innerHTML = "Top of the Rock Observation Deck: " + "Start: " + startTop + " End: " + endTop
    }
    if (startThe != "0"){
      document.getElementById("startThe").innerHTML = "The Metropolitan Museum of Art: " + "Start: " + startThe + " End: " + endThe
    }
    if (startTimes != "0"){
      document.getElementById("startTimes").innerHTML = "Times Square: " + "Start: " + startTimes + " End: " + endTimes
    }
    if (startFive != "0"){
      document.getElementById("startFive").innerHTML = "5 Napkin Burger: " + "Start: " + startFive + " End: " + endFive
    }
    if (startJuliana != "0"){
      document.getElementById("startJuliana").innerHTML = "Juliana's Pizza: " + "Start: " + startJuliana + " End: " + endJuliana 
    }
    if (startBleecker != "0"){
      document.getElementById("startBleecker").innerHTML = "Bleecker Street Pizza: " + "Start: " + startBleecker + " End: " + endBleecker
    }
    if (startAmélie != "0"){
      document.getElementById("startAmélie").innerHTML = "Amélie: " + "Start: " + startAmélie + " End: " + endAmélie
    }
    if (startKeens != "0"){
      document.getElementById("startKeens").innerHTML = "Keens Steakhouse: " + "Start: " + startKeens + " End: " + endKeens
    }
    if (startBergdorf != "0"){
      document.getElementById("startBergdorf").innerHTML = "Bergdorf Goodman: " + "Start: " + startBergdorf + " End: " + endBergdorf
    }
    if (startGrand != "0"){
      document.getElementById("startGrand").innerHTML = "Grand Central Market: " + "Start: " + startGrand + " End: " + endGrand
    }
    if (startFifth != "0"){
      document.getElementById("startFifth").innerHTML = "Fifth Avenue Street: " + "Start: " + startFifth + " End: " + endFifth
    }
  }
</script>
