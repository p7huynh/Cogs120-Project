<p id="endDiver"></p>
<p id="startShin"></p>
<p id="endShin"></p>

<script type="text/javascript">
  // variables for Tokyo Activities
  var startEbisuya = {{Ebisuya}};
  var endEbisuya = {{endEbisuya}};
  var starttokyotour = {{tokyotour}};
  var endtokyotour = {{endtokyotour}};
  var startShinjuku = {{Shinjuku}};
  var endShinjuku = {{endShinjuku}};
  var startFuji = {{Fuji}};
  var endFuji = {{endFuji}};
  var startRyogoku = {{Ryogoku}};
  var endRyogoku = {{endRyogoku}};
  // Tokyo Food
  var startIchiran = {{Ichiran}};
  var endIchiran = {{endIchiran}};
  var startNabezo = {{Nabezo}};
  var endNabezo = {{endNabezo}};
  var startDaiwa = {{Daiwa}};
  var endDaiwa = {{endDaiwa}};
  var startIse = {{Ise}};
  var endIse = {{endIse}};
  var startHan = {{Han}};
  var endHan = {{endHan}};
  //Tokyo Shopping
  var startIsetan = {{Isetan}};
  var endIsetan = {{endIsetan}};
  var startDiver = {{Diver}};
  var endDiver = {{endDiver}};
  var startShin = {{Shin}};
  var endShin = {{endShin}};

  function test() {
    if (startEbisuya != "0"){
      document.getElementById("startEbisuya").innerHTML = "Ebisuya Asakusa: " + "Start: " + hourconvert(startEbisuya) + " End: " + hourconvert(endEbisuya)
    }
    if (starttokyotour != "0"){
      document.getElementById("starttokyotour").innerHTML = "Tokyo FoodDrink Tour: " + "Start: " + hourconvert(starttokyotour) + " End: " + hourconvert(endtokyotour)
    }
    if (startShinjuku != "0"){
      document.getElementById("startShinjuku").innerHTML = "Shinjuku Gyoen National Garden: " + "Start: " + hourconvert(startShinjuku) + " End: " + hourconvert(endShinjuku)
    }
    if (startFuji != "0"){
      document.getElementById("startFuji").innerHTML = "Mt. Fuji: " + "Start: " + hourconvert(startFuji) + " End: " + hourconvert(endFuji)
    }
    if (startRyogoku != "0"){
      document.getElementById("startRyogoku").innerHTML = "Ryogoku Kokugikan (Sumo): " + "Start: " + hourconvert(startRyogoku) + " - End: " + hourconvert(endRyogoku)
    }
    if (startIchiran != "0"){
      document.getElementById("startIchiran").innerHTML = "Ichiran Shibuya (Ramen): " + "Start: " + hourconvert(startIchiran) + " - End: " + hourconvert(endIchiran)
    }
    if (startNabezo != "0"){
      document.getElementById("startNabezo").innerHTML = "Nabezo Shinjuku East Entrance (BBQ): " + "Start: " + hourconvert(startNabezo) + " - End: " + hourconvert(endNabezo )
    }
    if (startDaiwa != "0"){
      document.getElementById("startDaiwa").innerHTML = "Daiwa Sushi: " + "Start: " + hourconvert(startDaiwa) + " - End: " + hourconvert(endDaiwa)
    }
    if (startIse != "0"){
      document.getElementById("startIse").innerHTML = "Ise Sueyoshi: " + "Start: " + hourconvert(startIse) + " - End: " + hourconvert(endIse)
    }
    if (startHan != "0"){
      document.getElementById("startHan").innerHTML = "Han no Daidokoro Kadochika: " + "Start: " + hourconvert(startHan) + " - End: " + hourconvert(endHan)
    }
    if (startIsetan != "0"){
      document.getElementById("startIsetan").innerHTML = "Isetan Shinjuku Store: " + "Start: " + hourconvert(startIsetan) + " - End: " + hourconvert(endIsetan)
    }
    if (startDiver != "0"){
      document.getElementById("startDiver").innerHTML = "Diver City Tokyo Plaza: " + "Start: " + hourconvert(startDiver) + " - End: " + hourconvert(endDiver)
    }
    if (startShin != "0"){
      document.getElementById("startShin").innerHTML = "Shin Nakamise Shopping Street: " + "Start: " + hourconvert(startShin) + " - End: " + hourconvert(endShin)
    }

  }

function hourconvert(hour){
  if (parseInt(hour) > 12) {
    var newtime = parseInt(hour) - 12;
    newtime = newtime.toString() + " PM";
  }
  else{
    var newtime = hour + " AM";
  }
  return newtime;
}
</script>

